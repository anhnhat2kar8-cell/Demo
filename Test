local a,b,c,d,e=game:GetService("Players"),game:GetService("RunService"),game:GetService("VirtualUser"),game:GetService("TweenService"),game:GetService("ReplicatedStorage")
local f=a.LocalPlayer
local g=Instance.new("ScreenGui")
g.Parent=game.CoreGui
g.ResetOnSpawn=false
local h=Instance.new("Frame")
h.Size=UDim2.new(0,220,0,280)
h.Position=UDim2.new(1,-240,0.3,0)
h.BackgroundColor3=Color3.fromRGB(20,20,20)
h.BackgroundTransparency=0.3
h.Active=true
h.Draggable=true
h.Parent=g
Instance.new("UICorner",h).CornerRadius=UDim.new(0,12)
local i={Color3.fromRGB(255,0,0),Color3.fromRGB(255,127,0),Color3.fromRGB(255,255,0),Color3.fromRGB(0,255,0),Color3.fromRGB(0,0,255),Color3.fromRGB(75,0,130),Color3.fromRGB(148,0,211)}
local j=Instance.new("UIStroke")
j.Thickness=4
j.Parent=h
j.Color=i[1]
task.spawn(function()
 local k=1
 while j.Parent do
  local l=k%#i+1
  local m=d:Create(j,TweenInfo.new(0.5,Enum.EasingStyle.Linear),{Color=i[l]})
  m:Play()
  m.Completed:Wait()
  k=l
 end
end)
local n=Instance.new("TextLabel")
n.Size=UDim2.new(1,0,0,25)
n.Position=UDim2.new(0,0,0,5)
n.BackgroundTransparency=1
n.Text="☠️ Mất Nhận Thức ☠️"
n.Font=Enum.Font.GothamBold
n.TextSize=18
n.TextColor3=Color3.fromRGB(255,255,255)
n.TextXAlignment=Enum.TextXAlignment.Center
n.TextYAlignment=Enum.TextYAlignment.Center
n.Parent=h

