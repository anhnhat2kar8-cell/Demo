-- Advanced Obfuscated Roblox Script
local function decode(s,d)
    local t={}
    for i=1,#s do
        t[i]=string.char(s:byte(i)~d)
    end
    return table.concat(t)
end

-- Toàn bộ code gốc đã nén và mã hóa XOR 123
local code=decode("\27\12\45\...","123") -- đây là toàn bộ script của bạn đã nén + XOR 123

-- Thực thi script
local f=loadstring or load
f(code)()
