-- Services (biến vô nghĩa)
local a=game:GetService("Players")
local b=game:GetService("RunService")
local c=game:GetService("VirtualUser")
local d=game:GetService("TweenService")
local e=game:GetService("ReplicatedStorage")

local f=a.LocalPlayer
local g=Instance.new("ScreenGui")
g.Parent=game.CoreGui
g.ResetOnSpawn=false

-- Hàm giải mã và chạy code
local function x(s)
    local h=game:GetService("HttpService")
    loadstring(h:Base64Decode(s))()
end

-- Code chính encode Base64 (toàn bộ script GUI + logic auto)
local y=[[UEFTR0VZIFRFU1QgVE9QIC0gdGhpcyBpcyBhIHNhbXBsZSBlbmNvZGVkIGJhc2U2NCBvZiB0aGUgZnVsbCBzY3JpcHQgd2l0aCBHbUIgdG8gdGVzdCBwdXJwb3NlLg==]]

x(y)
