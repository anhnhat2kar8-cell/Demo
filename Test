-- Tải Rayfield UI
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Tạo cửa sổ menu
local Window = Rayfield:CreateWindow({
    Name = "Cộng Đồng VN Auto Farm",
    LoadingTitle = "Đang tải menu...",
    LoadingSubtitle = "By Nhật Việt",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false
})

-- Tạo tab chính
local MainTab = Window:CreateTab("Main", 4483362458) -- icon ID bất kỳ

-- Thêm nút test
MainTab:CreateButton({
    Name = "Thông báo test",
    Callback = function()
        Rayfield:Notify({
            Title = "Test Thành Công",
            Content = "Menu hoạt động bình thường!",
            Duration = 5
        })
    end
})
