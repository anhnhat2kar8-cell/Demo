-- Mã hóa cao cấp: Base64 + XOR + loadstring
local function xorDecode(data, key)
    local result = {}
    for i = 1, #data do
        local c = string.byte(data, i)
        local k = string.byte(key, (i - 1) % #key + 1)
        result[i] = string.char(bit32.bxor(c, k))
    end
    return table.concat(result)
end

local encoded = [[
<CHÈN_BASE64_CỦA_SCRIPT_BẠN_ĐÃ_XOR>
]]

local key = "M4stNh4nThuc@2025" -- key bí mật, giữ kín

local decodedBase64 = game:GetService("HttpService"):Base64Decode(encoded)
local decodedScript = xorDecode(decodedBase64, key)
load(decodedScript)()
