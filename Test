-- Ultra Obfuscated Roblox Script (All-in-One)
local function xor(s,key)
    local t={}
    for i=1,#s do t[i]=string.char(s:byte(i)~key) end
    return table.concat(t)
end

-- Toàn bộ script gốc đã nén + XOR 123
local code = xor(
"\27\12\45\61\34\56\78\90\12\34\56\..." -- chuỗi dài chứa toàn bộ script của bạn
,123)

-- Thực thi
local run = loadstring or load
run(code)()
